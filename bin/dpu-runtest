from dpu.suite import TestSuite
from dpu.utils import mkdir, tmpdir
import sys

if len(sys.argv) <= 1:
    print "Run what? :)"
    sys.exit(1)

ws = TestSuite(".")
test = ws.get_test(sys.argv[1])

print "%s: Running test -- %s" % ("dpu", test.name)
val = test.run()
print "%s: Ran test     -- %s (%s)" % ("dpu", test.name, val)
